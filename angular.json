{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "biblio-front": {
      "projectType": "application",
      "schematics": {
       
        "@schematics/angular:component": {
             "type": "component"
        },
        "@schematics/angular:service": {
          "type": "service"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.css",
              "public/assets/css/bootstrap.min.css",
              "public/assets/css/bootstrap-extended.css",
              "public/assets/css/style.css"

            ],
            "scripts": [
              "public/assets/js/jquery.min.js",
              "public/assets/js/bootstrap.bundle.min.js",
              "public/assets/plugins/simplebar/js/simplebar.min.js",
              "public/assets/plugins/metismenu/js/metisMenu.min.js",
              "public/assets/plugins/datatable/js/jquery.dataTables.min.js",
              "public/assets/plugins/datatable/js/dataTables.bootstrap5.min.js",
              "public/assets/plugins/perfect-scrollbar/js/perfect-scrollbar.js",
              "public/assets/js/pace.min.js",
              "public/assets/plugins/vectormap/jquery-jvectormap-2.0.2.min.js",
              "public/assets/plugins/vectormap/jquery-jvectormap-world-mill-en.js",
              "public/assets/plugins/apexcharts-bundle/js/apexcharts.min.js",
              "public/assets/js/app.js",
              "public/assets/js/index.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "biblio-front:build:production"
            },
            "development": {
              "buildTarget": "biblio-front:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "scripts": [
              "public/assets/js/jquery.min.js",
              "public/assets/js/bootstrap.bundle.min.js",
              "public/assets/plugins/simplebar/js/simplebar.min.js",
              "public/assets/plugins/perfect-scrollbar/js/perfect-scrollbar.js",
              "public/assets/js/pace.min.js",
              "public/assets/plugins/vectormap/jquery-jvectormap-2.0.2.min.js",
              "public/assets/plugins/vectormap/jquery-jvectormap-world-mill-en.js",
              "public/assets/plugins/apexcharts-bundle/js/apexcharts.min.js",
              "public/assets/js/app.js",
              "public/assets/js/index.js"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "dabe4342-4527-4b28-a264-0d1347fd835b"
  }
}
